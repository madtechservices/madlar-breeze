{
    "name": "tomatophp/tomato-breeze",
    "description": "Laravel Breeze starter kit for Splade build with RTL/Dark for TomatoPHP",
    "keywords": [
        "laravel",
        "auth",
        "splade",
        "laravel-splade"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tomatophp/tomato-breeze/issues",
        "source": "https://github.com/tomatophp/tomato-breeze"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media"
        },
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/console": "^9.37|^10.0",
        "illuminate/filesystem": "^9.37|^10.0",
        "illuminate/support": "^9.37|^10.0",
        "illuminate/validation": "^9.37|^10.0",
        "tomatophp/tomato-splade": "^1.0"
    },
    "conflict": {
        "laravel/framework": "<9.37.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Breeze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.1"
    }
}
